{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    },
    "KenticoEventLog": {
      "LogLevel": {
        "Default": "Error",
        "Microsoft.AspNetCore.Server.Kestrel": "None"
      }
    }
  },
  "AllowedHosts": "*",
  "CMSHashStringSalt": "",
  "ConnectionStrings": {
    "CMSConnectionString": ""
  },
  "WebCrawlerBaseUrl": "http://localhost:58671/",
  "RobotsTxtDenyAll": true,
  "XbKFeatureFlags": {
    "CacheInMinutes": 86400
  },
  "Vite": {
    "Base": "/",
    "PackageManager": "yarn",
    "PackageDirectory": "../KitchenCommandCenter.Web"
  },
  "VueSsr": {
    "Enabled": true,
    "BaseUrl": "http://localhost:3001"
  }
}
